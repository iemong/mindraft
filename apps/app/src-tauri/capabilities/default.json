{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"opener:default",
		"store:default",
		"dialog:default",
		"fs:default",
		{
			"identifier": "shell:allow-execute",
			"allow": [
				{
					"name": "binaries/server",
					"sidecar": true
				}
			]
		},
		"shell:allow-open"
	]
}
